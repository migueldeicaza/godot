sdi=swift/Sources/CGodot/include/

(find . -name '*.h' -and -not -name 'umbrella*.h' | grep -v '\.build' | grep -v "^./modules/" | grep -v "^./swift" | grep -v "^./platform" | grep -v thirdparty | grep -v "^./tests" | grep -v misc/dist/ios_xcode/godot_ios/dummy.h | grep -v ^./gdextension_interface.h | grep -v _wrap_mt | grep -v servers/display_server_headless.h| grep -v main/main | grep -v editor/builtin_fonts.gen.h | grep -v "^./drivers" | grep -v "^./servers/rendering" | grep -v gen.h$; echo ./core/disabled_classes.gen.h; echo core/version_generated.gen.h; echo modules/modules_enabled.gen.h; echo modules/regex/regex.h; echo servers/rendering_server.h; echo servers/rendering/rendering_device.h; echo servers/rendering/shader_warnings.h; echo servers/rendering/rendering_device_binds.h) | rsync -a --files-from=- ./ $sdi


mkdir -p $sdi/thirdparty/misc mkdir -p $sdi/thirdparty/minizip
cp ./platform/macos/platform_config.h $sdi/
cp core/string/char_range.inc swift/Sources/CGodot/include/core/string/
cp core/object/gdvirtual.gen.inc swift/Sources/CGodot/include/core/object/
cp core/extension/ext_wrappers.gen.inc swift/Sources/CGodot/include/core/extension/

cp thirdparty/misc/{r128.h,pcg.h,ok_color.h,mikktspace.h} $sdi/thirdparty/misc
cp thirdparty/minizip/{zip.h,unzip.h,ioapi.h} thirdparty/minizip/unzip.h $sdi/thirdparty/minizip